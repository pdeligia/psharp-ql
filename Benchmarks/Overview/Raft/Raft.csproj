      <HintPath>..\..\..\bin\net46\Microsoft.PSharp.dll</HintPath>
    </Reference>
    <Reference Include="Microsoft.PSharp.TestingServices">
      <HintPath>..\..\..\bin\net46\Microsoft.PSharp.TestingServices.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
